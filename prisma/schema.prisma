generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Todo {
  id        String   @id @default(uuid())
  title     String
  status    String   @default("incomplete")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
